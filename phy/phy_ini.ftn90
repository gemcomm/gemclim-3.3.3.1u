!** S/P phy_ini
      Subroutine phy_ini (n,nk)
!
#include "impnone.cdk"
      Integer n,nk
!
!Author
!          M. Desgagne (Oct 1995)
!
!Revision
! 001      L. Spacek  (Aug 2010) - Complete rewrite
! 002      B. Dugas   (Oct 2010) - A few small corrections
! 003      A. Martynov(Aug 2011) - Add 1D interactive lakes
! 004      B. Dugas   (Mar 2011) - Initialize ptdebut to a big_number
! 005      Zadra/McTaggart-Cowan (Jan 2012) - Account for eturbl9 mods
!
!Object
!          Establishes requirements in terms of variables in
!          the 4 main buses (busent, busdyn, busper and busvol)
!          for the entire physics.
!
!Arguments
!
!          - Input -
! n        horizontal dimension
! nk       vertical dimension
!
!Notes
! Sort Command
! sort -t, -k3,3f -k1,1 source > destination
!
!Implicites
!
#include "classlvls.cdk"
#include "nclassvg.cdk"
#include "icelvls.cdk"
#include "options.cdk"
#include "nbvarsurf.cdk"
#include "dimsurf.cdk"
#define DCLCHAR
#include "phy_macros_f.h"
#include "phybus.cdk"
#include "mcica.cdk"
#include "lakes.cdk"
!
!MODULES
      External PHYBUS_DATA
      Integer          :: nvarphy
      Integer(8)       :: big_number = 1234567890123456789_8
!
      Character(len=2) nagg, nrow, ncg, ncv, ncvp, nipt, nm, ninl
      Logical          :: lcn09_14,ncn09_14,lcn10_14,lcn11_14,lcn14,lcn05
      Logical          :: lbourg3d,lbourg
      Logical          :: lteb,lisba,lfcst,lclass,lccmave,lclass26,lfcis
      Logical          :: lsnala,nsnala
      Logical          :: lcpl
      Logical          :: lrslp
      Logical          :: lv4d
      Logical          :: lfcpkuo,nfcpkuo,lfckfbe,lkfbe
      Logical          :: ladvtke,nadvtke
      Logical          :: lmoyhr,lmoyhrkf
      Logical          :: liwf
      Logical          :: llakes

!     Initialize all elements of phybus_i to a negative big_number
      nvarphy = (loc(phybus_i_last)-loc(phybus_i_first(0))-1) &
              / (loc(phybus_i_first(0))-loc(phybus_i_first(-1)))
      phybus_i_first(-1:nvarphy) = - ( big_number )
!
!     nagg is the dimension of aggregrated variables
      Write(nagg,'(i2)') nsurf+1
!
!     nl is the number of levels in sea ice
      Write(nrow,'(i2)') nl
!
!     ncg is the number of ground layers (CLASS)
      Write(ncg,'(i2)') class_IG
!
!     ncv is the number of special vegetation classes (CLASS)
      Write(ncv,'(i2)') class_IC
!
      Write(ncvp,'(i2)') (class_IC+1)
!
!     nipt is the number of tau/cloud top pressure bins in ISCCP histograms
      Write(nipt,'(i2)') ntau*nptop
!
!     nm is the number of mosaic 'layers'
      Write(nm,'(i2)') nmos
!
!     ninl is the number of lake 'layers'
      write(ninl,'(i2)') inl
!
      lcn09_14=.False.; lcn10_14=.False.; lcn11_14=.False.; lcn14=.False.
      lbourg3d=.False.
      lteb    =.False.;lisba   =.False.;lclass  =.False.;
      llakes  =.False.

      lcn05    =(istcond.Eq.5)
      lcn09_14=(istcond.Ge.9 .And. istcond.Le.14)
      ncn09_14=.Not.lcn09_14
      lcn10_14=(istcond.Ge.10.And. istcond.Le.14)
      lcn11_14=(istcond.Ge.11.And. istcond.Le.14)
      lcn14   =(istcond.Eq.14)
      lbourg3d=(pcptype.Eq.'BOURGE3D')
      lteb    =(schmurb.Eq.'TEB')
      lisba   =(schmsol.Eq.'ISBA')
      lclass  =(schmsol.Eq.'CLASS')
      lfcst   =(schmsol.Eq.'FCREST')
      lccmave =(lclass.And.vege_fields.Eq.'CCCMA')
      lsnala  =(snoalb_anl.And.lisba)
      nsnala  =(.not.snoalb_anl.And.lisba)
      lclass26=(nclass.Eq.26)
      lcpl    =coupling
      lrslp   =radslope
      lv4d    =(lin_v4d.Gt.0)
      lfcpkuo =(convec.Eq.'FCPKUO' .Or. convec.Eq.'KFCKUO2')
      nfcpkuo =.Not.lfcpkuo
      ladvtke =advectke
      nadvtke=.Not.ladvtke
      lfcis   =lfcst.Or.lisba
      lmoyhr  =(moyhr.Gt.0)
      lfckfbe = (convec.Eq.'FCP'.Or.convec.Eq.'KFC'.Or.convec.Eq.'FCPKUO'.Or.convec.Eq.'KFCKUO2'.Or.convec.Eq.'BECHTOLD')
      lkfbe   = (convec.Eq.'KFC'.Or.convec.Eq.'KFCKUO2'.Or.convec.Eq.'BECHTOLD')
      lmoyhrkf=(lmoyhr.And.lkfbe)
      lbourg  = (pcptype.Eq.'BOURGE'.Or.pcptype.Eq.'NIL')
      liwf    = (lclass.And.iwf.Gt.0)
      llakes  = (lakes.Ne.'NIL'.Or.ilakes.Ne.0)
!
 If(lisba)     Call gesdict (n, nk, acoef,        'VN=acoef        ;ON=1I  ;VD=a coef. in wgeq                                ;VS=row                    ;VB=p0')
               Call gesdict (n, nk, ae,           'VN=ae           ;ON=AE  ;VD=accum. of stratiform precip.                   ;VS=row                    ;VB=p0')
               Call gesdict (n, nk, aip,          'VN=aip          ;ON=PE  ;VD=accum. of refrozen precip.                     ;VS=row                    ;VB=p0')
 If(lbourg3d)  Call gesdict (n, nk, aip3d,        'VN=aip3d        ;ON=PE3D;VD=accum. of refrozen precip.3D                   ;VS=slb                    ;VB=p0')
 If(lteb)      Call gesdict (n, nk, alb_road,     'VN=alb_road     ;ON=2N  ;VD=road albedo                                    ;VS=row                    ;VB=p0')
 If(lteb)      Call gesdict (n, nk, alb_roof,     'VN=alb_roof     ;ON=1N  ;VD=roof albedo                                    ;VS=row                    ;VB=p0')
 If(lteb)      Call gesdict (n, nk, alb_wall,     'VN=alb_wall     ;ON=3N  ;VD=wall albedo                                    ;VS=row                    ;VB=p0')
               Call gesdict (n, nk, alc,          'VN=alc          ;ON=A1  ;VD=accum. of liquid convect. precip.              ;VS=row                    ;VB=p0')
               Call gesdict (n, nk, alcs,         'VN=alcs         ;ON=A8  ;VD=accum. of liquid shall. convect. precip.       ;VS=row                    ;VB=p0')
               Call gesdict (n, nk, alen,         'VN=alen         ;ON=1A  ;VD=visible albedo (E)                             ;VS=row                    ;VB=e1')
               Call gesdict (n, nk, alfaq,        'VN=alfaq        ;ON=AQ  ;VD=inhomog. term for Q diff.                      ;VS=row@'//nm//'           ;VB=v0')
               Call gesdict (n, nk, alfat,        'VN=alfat        ;ON=AT  ;VD=inhomog. term for T diffu.                     ;VS=row@'//nm//'           ;VB=v0')
 If(lclass)    Call gesdict (n, nk, algdry,       'VN=algdry       ;ON=B1  ;VD=albedo of dry soil                             ;VS=row@'//nm//'           ;VB=p0')
 If(lclass)    Call gesdict (n, nk, algwet,       'VN=algwet       ;ON=R7  ;VD=albedo of wet soil                             ;VS=row@'//nm//'           ;VB=p0')
 If(lclass)    Call gesdict (n, nk, alirc,        'VN=alirc        ;ON=C2  ;VD=canopy albedo (near i.r.)                      ;VS=row*'//ncvp//'@'//nm//';VB=p0')
 If(lccmave)   Call gesdict (n, nk, alircen,      'VN=alircen      ;ON=EAIC;VD=canopy albedo (near i.r.) (E)                  ;VS=row*'//ncvp//'         ;VB=e1')
               Call gesdict (n, nk, alnosno,      'VN=alnosno      ;ON=IA  ;VD=initial background albedo                      ;VS=row                    ;VB=p0')
               Call gesdict (n, nk, als,          'VN=als          ;ON=A2  ;VD=accum. of liquid precip, grid-scale cond       ;VS=row                    ;VB=p0')
 If(lcn10_14)  Call gesdict (n, nk, als_fr1,      'VN=als_fr1      ;ON=FR1 ;VD=accum. freezing drizzle      (M-Y)             ;VS=row                    ;VB=p0')
 If(lcn10_14)  Call gesdict (n, nk, als_fr2,      'VN=als_fr2      ;ON=FR2 ;VD=accum. freezing rain         (M-Y)             ;VS=row                    ;VB=p0')
 If(lcn10_14)  Call gesdict (n, nk, als_rn1,      'VN=als_rn1      ;ON=RN1 ;VD=accum. liquid drizzle        (M-Y)             ;VS=row                    ;VB=p0')
 If(lcn10_14)  Call gesdict (n, nk, als_rn2,      'VN=als_rn2      ;ON=RN2 ;VD=accum. liquid rain           (M-Y)             ;VS=row                    ;VB=p0')
 If(lisba)     Call gesdict (n, nk, alveg,        'VN=alveg        ;ON=AX  ;VD=visible canopy albedo                          ;VS=row                    ;VB=p0')
               Call gesdict (n, nk, alvis,        'VN=alvis        ;ON=AL  ;VD=visible surface albedo                         ;VS=row*'//nagg//'@'//nm//';VB=p0')
 If(lclass)    Call gesdict (n, nk, alvsc,        'VN=alvsc        ;ON=C4  ;VD=canopy albedo (visible)                        ;VS=row*'//ncvp//'@'//nm//';VB=p0')
 If(lccmave)   Call gesdict (n, nk, alvscen,      'VN=alvscen      ;ON=EAVC;VD=canopy albedo (visible) (E)                    ;VS=row*'//ncvp//'         ;VB=e1')
               Call gesdict (n, nk, ap,           'VN=ap           ;ON=AP  ;VD=planetary albedo                               ;VS=row                    ;VB=v0')
 If(lfckfbe)   Call gesdict (n, nk, areaup,       'VN=areaup       ;ON=UA  ;VD=area of updraft (FCP)                          ;VS=slb                    ;VB=p0')
               Call gesdict (n, nk, asc,          'VN=asc          ;ON=A3  ;VD=accum. of solid  convect. precip.              ;VS=row                    ;VB=p0')
               Call gesdict (n, nk, ascs,         'VN=ascs         ;ON=A9  ;VD=accum. of solid  shall. convect. precip.       ;VS=row                    ;VB=p0')
               Call gesdict (n, nk, ass,          'VN=ass          ;ON=A4  ;VD=accum. of total solid precip, grid-scale cond  ;VS=row                    ;VB=p0')
 If(lcn10_14)  Call gesdict (n, nk, ass_mx,       'VN=ass_mx       ;ON=AMX ;VD=accum. mixed pcp              (M-Y)            ;VS=row                    ;VB=p0')
 If(lcn10_14)  Call gesdict (n, nk, ass_pe1,      'VN=ass_pe1      ;ON=PE1 ;VD=accum. ice pellets           (M-Y)             ;VS=row                    ;VB=p0')
 If(lcn10_14)  Call gesdict (n, nk, ass_pe2,      'VN=ass_pe2      ;ON=PE2 ;VD=accum. hail (M-Y)                              ;VS=row                    ;VB=p0')
 If(lcn10_14)  Call gesdict (n, nk, ass_pe2l,     'VN=ass_pe2l     ;ON=PE2L;VD=accum. large hail          (M-Y)               ;VS=row                    ;VB=p0')
 If(lcn10_14)  Call gesdict (n, nk, ass_s2l,      'VN=ass_s2l      ;ON=S2L ;VD=solid:liquid ratio [SND/SN]  (M-Y)             ;VS=row                    ;VB=p0')
 If(lcn10_14)  Call gesdict (n, nk, ass_sn1,      'VN=ass_sn1      ;ON=SN1 ;VD=accum. ice crystals          (M-Y)             ;VS=row                    ;VB=p0')
 If(lcn10_14)  Call gesdict (n, nk, ass_sn2,      'VN=ass_sn2      ;ON=SN2 ;VD=accum. snow (M-Y)                              ;VS=row                    ;VB=p0')
 If(lcn10_14)  Call gesdict (n, nk, ass_sn3,      'VN=ass_sn3      ;ON=SN3 ;VD=accum. graupel (M-Y)                           ;VS=row                    ;VB=p0')
 If(lcn10_14)  Call gesdict (n, nk, ass_snd,      'VN=ass_snd      ;ON=SND ;VD=accum. total unmelted snow (micro)             ;VS=row                    ;VB=p0')
               Call gesdict (n, nk, at2e,         'VN=at2e         ;ON=AT2E;VD=weights T,Q to enrg                            ;VS=slb                    ;VB=v0')
               Call gesdict (n, nk, at2m,         'VN=at2m         ;ON=AT2M;VD=weights T,Q to mom                             ;VS=slb                    ;VB=v0')
               Call gesdict (n, nk, at2t,         'VN=at2t         ;ON=AT2T;VD=weights T,Q to thermo                          ;VS=slb                    ;VB=v0')
 If(lteb)      Call gesdict (n, nk, azim,         'VN=azim         ;ON=8X  ;VD=solar azimuthal angle                          ;VS=row                    ;VB=p0')
               Call gesdict (n, nk, azr,          'VN=azr          ;ON=FR  ;VD=accum. of freezg precip.                       ;VS=row                    ;VB=p0')
 If(lbourg3d)  Call gesdict (n, nk, azr3d,        'VN=azr3d        ;ON=FR3D;VD=accum. of freezg precip. 3D                    ;VS=slb                    ;VB=p0')
               Call gesdict (n, nk, basc,         'VN=basc         ;ON=BS  ;VD=base sigma of condensation level               ;VS=row                    ;VB=v0')
 If(lclass)    Call gesdict (n, nk, bbi,          'VN=bbi          ;ON=C6  ;VD=Clapp and Hornberger hydraulic coefficient B   ;VS=row*'//ncg//'@'//nm//' ;VB=p0')
 If(lisba)     Call gesdict (n, nk, bcoef,        'VN=bcoef        ;ON=1G  ;VD=slope of retention curve                       ;VS=row                    ;VB=p0')
 If(lteb)      Call gesdict (n, nk, bld,          'VN=bld          ;ON=1T  ;VD=building fraction                              ;VS=row                    ;VB=p0')
 If(lteb)      Call gesdict (n, nk, bld_height,   'VN=bld_height   ;ON=3T  ;VD=building height                                ;VS=row                    ;VB=p0')
               Call gesdict (n, nk, bm,           'VN=bm           ;ON=BM  ;VD=homog. term for U,V diffu.                     ;VS=row@'//nm//'           ;VB=v0')
 If(lv4d)      Call gesdict (n, nk, bmm,          'VN=bmm          ;ON=7M  ;VD=traj of bm                                     ;VS=row                    ;VB=p0')
               Call gesdict (n, nk, bt,           'VN=bt           ;ON=BT  ;VD=homog. term for T,Q diffu.                     ;VS=row*'//nagg//'@'//nm//';VB=v0')
 If(lv4d)      Call gesdict (n, nk, btm,          'VN=btm          ;ON=7T  ;VD=traj of bt                                     ;VS=row                    ;VB=p0')
 If(lisba)     Call gesdict (n, nk, c1sat,        'VN=c1sat        ;ON=3I  ;VD=c1 coef. at saturation                         ;VS=row                    ;VB=p0')
 If(lrslp)     Call gesdict (n, nk, c1slop,       'VN=C1SL         ;ON=C1SL;VD=C1 for radslope                                ;VS=row                    ;VB=p0')
 If(lisba)     Call gesdict (n, nk, c2ref,        'VN=c2ref        ;ON=4I  ;VD=reference value of c2                          ;VS=row                    ;VB=p0')
 If(lrslp)     Call gesdict (n, nk, c2slop,       'VN=C2SL         ;ON=C2SL;VD=C2 for radslope                                ;VS=row                    ;VB=p0')
 If(lisba)     Call gesdict (n, nk, c3ref,        'VN=c3ref        ;ON=5I  ;VD=drainage coef. to deeper soil                  ;VS=row                    ;VB=p0')
 If(lrslp)     Call gesdict (n, nk, c3slop,       'VN=C3SL         ;ON=C3SL;VD=C3 for radslope                                ;VS=row                    ;VB=p0')
 If(lrslp)     Call gesdict (n, nk, c4slop,       'VN=C4SL         ;ON=C4SL;VD=C4 for radslope                                ;VS=row                    ;VB=p0')
 If(lrslp)     Call gesdict (n, nk, c5slop,       'VN=C5SL         ;ON=C5SL;VD=C5 for radslope                                ;VS=row                    ;VB=p0')
 If(lteb)      Call gesdict (n, nk, can_hw_ratio, 'VN=can_hw_ratio ;ON=6T  ;VD=aspect ratio of the street                     ;VS=row                    ;VB=p0')
               Call gesdict (n, nk, cang,         'VN=cang         ;ON=CX  ;VD=cosine of the solar angle                      ;VS=row                    ;VB=v0')
 If(lkfbe)     Call gesdict (n, nk, capekfc,      'VN=capekfc      ;ON=U4  ;VD=CAPE calculated in KF                          ;VS=row                    ;VB=p0')
 If(lmoyhrkf)  Call gesdict (n, nk, capekfcm,     'VN=capekfcm     ;ON=6K  ;VD=avg capekfc                                    ;VS=row                    ;VB=p0')
 If(lmoyhr)    Call gesdict (n, nk, ccnm,         'VN=ccnm         ;ON=CA  ;VD=total clouds avge over last moyhr hrs          ;VS=slb                    ;VB=p0')
 If(lclass)    Call gesdict (n, nk, cdh,          'VN=cdh          ;ON=G2  ;VD=bulk heat transfer coefficient                 ;VS=row@'//nm//'           ;VB=v0')
 If(lclass)    Call gesdict (n, nk, cdm,          'VN=cdm          ;ON=CM  ;VD=bulk momentum transfer coefficient             ;VS=row@'//nm//'           ;VB=v0')
               Call gesdict (n, nk, cgsat,        'VN=cgsat        ;ON=6I  ;VD=thermal coef. at saturation                    ;VS=row                    ;VB=p0')
 If(lclass)    Call gesdict (n, nk, clay,         'VN=clay         ;ON=CLAY;VD=percentage of clay in soil                     ;VS=row*'//ncg//'          ;VB=p0')
 If(lisba)     Call gesdict (n, nk, clay,         'VN=clay         ;ON=J2  ;VD=percentage of clay in soil                     ;VS=row*3                  ;VB=p0')
 If(lclass)    Call gesdict (n, nk, clayen,       'VN=clayen       ;ON=2H  ;VD=perc. of clay in soil (E)                      ;VS=row*'//ncg//'          ;VB=e1')
 If(lisba)     Call gesdict (n, nk, clayen,       'VN=clayen       ;ON=2H  ;VD=perc. of clay in soil (E)                      ;VS=row*3                  ;VB=e1')
               Call gesdict (n, nk, clb,          'VN=clb          ;ON=CLB ;VD=net clear sky lw flux at sfc                   ;VS=row                    ;VB=p0')
               Call gesdict (n, nk, clbaf,        'VN=clbaf        ;ON=ACLB;VD=accumulation of clb                            ;VS=row                    ;VB=p0')
               Call gesdict (n, nk, cldrad,       'VN=cldrad       ;ON=CLDR;VD=cloud for radiation                            ;VS=slb                    ;VB=v0')
 If(lmoyhr)    Call gesdict (n, nk, cldradm,      'VN=cldradm      ;ON=CLRM;VD=total clouds for rad avg over last moyhr hours ;VS=slb                    ;VB=p0')
               Call gesdict (n, nk, clr,          'VN=clr          ;ON=13  ;VD=cloud rain kg/m3 (consun)                      ;VS=slb                    ;VB=v0')
               Call gesdict (n, nk, cls,          'VN=cls          ;ON=NN  ;VD=cloud snow kg/m3 (consun)                      ;VS=slb                    ;VB=v0')
               Call gesdict (n, nk, clt,          'VN=clt          ;ON=CLT ;VD=net clear sky lw flux at top                   ;VS=row                    ;VB=p0')
               Call gesdict (n, nk, cltaf,        'VN=cltaf        ;ON=ACLT;VD=accumulation of clt                            ;VS=row                    ;VB=p0')
 If(lclass)    Call gesdict (n, nk, cmai,         'VN=cmai         ;ON=CY  ;VD=instantaneous canopy mass                      ;VS=row@'//nm//'           ;VB=p0')
               Call gesdict (n, nk, cosas,        'VN=cosas        ;ON=CO  ;VD=cosine of the solar angle for averaging        ;VS=row                    ;VB=p0')
               Call gesdict (n, nk, cosz,         'VN=cosz         ;ON=M4  ;VD=cosine of the solar angle                      ;VS=row                    ;VB=p0')
               Call gesdict (n, nk, covf,         'VN=covf         ;ON=6F  ;VD=cover fractions                                ;VS=row*38                 ;VB=p0')
               Call gesdict (n, nk, cqe,          'VN=cqe          ;ON=CQE ;VD=conv. Q tendency                               ;VS=slb                    ;VB=v0')
 If(lfcst)     Call gesdict (n, nk, cs,           'VN=cs           ;ON=HC  ;VD=soil heat capacity                             ;VS=row                    ;VB=v0')
               Call gesdict (n, nk, csb,          'VN=csb          ;ON=CSB ;VD=net clear sky solar flux at sfc                ;VS=row                    ;VB=p0')
               Call gesdict (n, nk, csbaf,        'VN=csbaf        ;ON=ACSB;VD=accumulation of csb                            ;VS=row                    ;VB=p0')
               Call gesdict (n, nk, cstaf,        'VN=cstaf        ;ON=ACST;VD=accumulation of cstt                           ;VS=row                    ;VB=p0')
               Call gesdict (n, nk, cstt,         'VN=cstt         ;ON=CSTT;VD=net clear sky solar flux at top                ;VS=row                    ;VB=p0')
               Call gesdict (n, nk, cte,          'VN=cte          ;ON=CTE ;VD=conv. T tendency                               ;VS=slb                    ;VB=v0')
               Call gesdict (n, nk, ctp,          'VN=ctp          ;ON=BP  ;VD=cloud top pressure                             ;VS=row                    ;VB=v0')
               Call gesdict (n, nk, ctt,          'VN=ctt          ;ON=BE  ;VD=cloud top temperature                          ;VS=row                    ;VB=v0')
               Call gesdict (n, nk, ctue,         'VN=ctue         ;ON=CH  ;VD=transfert coeff. for thermo.(m/s)              ;VS=row                    ;VB=v0')
 If(lisba)     Call gesdict (n, nk, cveg,         'VN=cveg         ;ON=CV  ;VD=thermal coefficient for canopy                 ;VS=row                    ;VB=p0')
 If(lteb)      Call gesdict (n, nk, d_road,       'VN=d_road       ;ON=3O  ;VD=depth of the road layers                       ;VS=row*3                  ;VB=p0')
 If(lteb)      Call gesdict (n, nk, d_roof,       'VN=d_roof       ;ON=9N  ;VD=depth of the roof layers                       ;VS=row*3                  ;VB=p0')
 If(lteb)      Call gesdict (n, nk, d_wall,       'VN=d_wall       ;ON=6O  ;VD=depth of the wall layers                       ;VS=row*3                  ;VB=p0')
 If(lclass)    Call gesdict (n, nk, delzw,        'VN=delzw        ;ON=C7  ;VD=thickness of soil layers for water             ;VS=row*'//ncg//'@'//nm//' ;VB=p0')
               Call gesdict (n, nk, dhdx,         'VN=dhdx         ;ON=Y7  ;VD=unresolved topo corr tensor(X)                 ;VS=row                    ;VB=p0')
