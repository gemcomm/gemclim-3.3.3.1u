 SUBROUTINE MULTIMOMENT_EXP1(W_omega,T,Q,QC,QR,QI,QN,QG,QH,NC,NR,NY,NN,NG,NH,PS,TM,    &
     QM,QCM,QRM,QIM,QNM,QGM,QHM,NCM,NRM,NYM,NNM,NGM,NHM,PSM,S,RT_rn,RT_dr,RT_fr,RT_fd, &
     RT_ic,RT_sg,RT_sn,RT_sp,RT_ip,RT_h1,RT_h2,GZ,T_TEND,Q_TEND,QCTEND,QRTEND,QITEND,  &
     QNTEND,QGTEND,QHTEND,NCTEND,NRTEND,NYTEND,NNTEND,NGTEND,NHTEND,dt,NI,N,NK,J,STEP, &
     CCNtype,Dm_c,Dm_r,Dm_i,Dm_s,Dm_g,Dm_h,ZET,SLW,VIS,SS_01,SS_02,SS_03,SS_04,SS_05,  &
     SS_06,SS_07,SS_08,SS_09,SS_10)

  USE MM_FUNCTIONS
  USE MM_SEDIMENTATION

  IMPLICIT NONE

! CALLING PARAMETERS:

  integer,                intent(IN)    :: NI,NK,N,J,STEP,CCNtype
  real,                   intent(IN)    :: dt
  real, dimension(ni),    intent(INOUT) :: PS,PSM
  real, dimension(ni),    intent(OUT)   :: RT_rn,RT_dr,RT_fr,RT_fd,RT_ic,RT_sg,      &
                                           RT_sn,RT_sp,RT_ip,RT_h1,RT_h2
  real, dimension(ni,nk), intent(IN)    :: W_omega,S,GZ
  real, dimension(ni,nk), intent(INOUT) :: T,Q,QC,QR,QI,QN,QG,QH,NC,NR,NY,NN,NG,NH,  &
        TM,QM,QCM,QRM,QIM,QNM,QGM,QHM,NCM,NRM,NYM,NNM,NGM,NHM
